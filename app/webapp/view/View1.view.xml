<mvc:View 
    xmlns:mvc="sap.ui.core.mvc" 
    xmlns="sap.m" controllerName="cyb.com.project1.controller.View1" 
    displayBlock="true">
    <Page id="page" title="{i18n>title}">
        <content>
            <HBox>
                <!-- <Button text="Add List Item" press="onAddListItem"/> -->
                <List headerText="{i18n>listTitle}" items="{path:'viewModel>/Product'}" height="30rem" width="40rem">
                    <ObjectListItem title="{viewModel>Name}"  intro="{viewModel>Description}"/>
                </List>

                <List headerText="Local Express" items="{path:'localModel>/listData'}" height="30rem" width="40rem">
                    <ObjectListItem title="{localModel>first_name}"  intro="{localModel>email}"/>
                </List>
             </HBox>
        </content>
    </Page>
</mvc:View>